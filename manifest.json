{
    "manifest_version": 3,
    "name": "Event Countdown Timer",
    "version": "1.0",
    "description": "Track upcoming and past events with countdown timers",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  }
  }